{
    "name":         "dh/artis-package-manager-bundle",
    "type":         "symfony-bundle",
    "description":  "Allows to easily manage packages",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Digital-Holding/Traitor.git"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "kkszymanowski/traitor": "dev-add_interface_on_null_extends",
        "phpunit/phpunit": "^9.3"
    },
    "require-dev": {
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/finder": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DH\\ArtisPackageManagerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DH\\ArtisPackageManagerBundle\\Tests\\": "tests/"
        }
    }
}
