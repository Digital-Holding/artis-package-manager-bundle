{
    "name":         "dh/artis-package-manager-bundle",
    "type":         "symfony-bundle",
    "description":  "Allows to easily manage packages",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Digital-Holding/Traitor.git"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "kkszymanowski/traitor": "dev-retrieve_namespace_on_added_declare",
        "nikic/php-parser": "^4.9",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/finder": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DH\\ArtisPackageManagerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DH\\ArtisPackageManagerBundle\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
